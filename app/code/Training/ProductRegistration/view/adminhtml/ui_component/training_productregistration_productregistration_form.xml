<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">training_productregistration_productregistration_form.productregistration_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Training\ProductRegistration\Block\Adminhtml\ProductRegistration\Edit\BackButton"/>
			<button name="delete" class="Training\ProductRegistration\Block\Adminhtml\ProductRegistration\Edit\DeleteButton"/>
			<button name="save" class="Training\ProductRegistration\Block\Adminhtml\ProductRegistration\Edit\SaveButton"/>
			<button name="save_and_continue" class="Training\ProductRegistration\Block\Adminhtml\ProductRegistration\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>training_productregistration_productregistration_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>training_productregistration_productregistration_form.productregistration_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="productregistration_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="productregistration_form_data_source" class="Training\ProductRegistration\Model\ProductRegistration\DataProvider">
			<settings>
				<requestFieldName>productregistration_id</requestFieldName>
				<primaryFieldName>productregistration_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="dop" formElement="date" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">dop</label>
				<dataScope>dop</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="name" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="sku" formElement="textarea" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">sku</label>
				<dataScope>sku</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="contactno" formElement="textarea" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">contactno</label>
				<dataScope>contactno</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="vendor" formElement="textarea" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">vendor</label>
				<dataScope>vendor</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="email" formElement="textarea" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">email</label>
				<dataScope>email</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="address" formElement="textarea" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">ProductRegistration</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">address</label>
				<dataScope>address</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
