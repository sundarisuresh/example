<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="sundari_application_application" resource="default" engine="innodb" comment="sundari_application_application Table">
		<column xsi:type="int" name="application_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="application_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		<column name="email" nullable="true" xsi:type="text" comment="email"/>
	</table>
</schema>
