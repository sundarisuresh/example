<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="sundari" translate="label" sortOrder="999">
			<label>sundari</label>
		</tab>
		<section id="order" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>order</label>
			<tab>sundari</tab>
			<resource>Sundari_AutoCancel::config_sundari_autocancel</resource>
			<group id="autocancel" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>autocancel</label>
				<field id="time" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>time</label>
					<comment>enter in hours</comment>
				</field>
			</group>
		</section>
	</system>
</config>
