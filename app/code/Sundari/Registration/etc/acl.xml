<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Sundari_Registration::Registration" title="Registration" sortOrder="10">
					<resource id="Sundari_Registration::Registration_save" title="Save Registration" sortOrder="10"/>
					<resource id="Sundari_Registration::Registration_delete" title="Delete Registration" sortOrder="20"/>
					<resource id="Sundari_Registration::Registration_update" title="Update Registration" sortOrder="30"/>
					<resource id="Sundari_Registration::Registration_view" title="View Registration" sortOrder="40"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Sundari_Registration::config_sundari_registration" title="product"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
