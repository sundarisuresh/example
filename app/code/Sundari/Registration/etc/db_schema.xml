<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="sundari_registration_registration" resource="default" engine="innodb" comment="sundari_registration_registration Table">
		<column xsi:type="int" name="registration_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="registration_id"/>
		</constraint>
		<column name="dateofpurchase" nullable="true" xsi:type="text" comment="dateofpurchase"/>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		<column name="emailid" nullable="true" xsi:type="text" comment="emailid"/>
		<column name="vendorname" nullable="true" xsi:type="text" comment="vendorname"/>
		<column name="address" nullable="true" xsi:type="text" comment="address"/>
		<column name="productserialno" nullable="true" xsi:type="text" comment="productserialno"/>
	</table>
</schema>
