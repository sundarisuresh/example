<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Sundari_Suggestionbox::Suggestion" title="Suggestion" sortOrder="10">
					<resource id="Sundari_Suggestionbox::Suggestion_save" title="Save Suggestion" sortOrder="10"/>
					<resource id="Sundari_Suggestionbox::Suggestion_delete" title="Delete Suggestion" sortOrder="20"/>
					<resource id="Sundari_Suggestionbox::Suggestion_update" title="Update Suggestion" sortOrder="30"/>
					<resource id="Sundari_Suggestionbox::Suggestion_view" title="View Suggestion" sortOrder="40"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Sundari_Suggestionbox::config_sundari_suggestionbox" title="search"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
