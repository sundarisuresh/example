<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="sundari_suggestionbox_suggestion" resource="default" engine="innodb" comment="sundari_suggestionbox_suggestion Table">
		<column xsi:type="int" name="suggestion_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="suggestion_id"/>
		</constraint>
		<column name="suggestion" nullable="true" xsi:type="text" comment="suggestion"/>
		<column name="customerid" nullable="true" xsi:type="text" comment="customerid"/>
		<column name="dateandtime" nullable="true" xsi:type="text" comment="dateandtime"/>
	</table>
</schema>
