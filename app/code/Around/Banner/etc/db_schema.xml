<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="around_banner_banner" resource="default" engine="innodb" comment="around_banner_banner Table">
		<column xsi:type="int" name="banner_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="banner_id"/>
		</constraint>
		<column name="image" nullable="true" xsi:type="text" comment="image"/>
		<column name="sortorder" nullable="true" xsi:type="text" comment="sortorder"/>
		<column name="alttext" nullable="true" xsi:type="text" comment="alttext"/>
		<column name="link" nullable="true" xsi:type="text" comment="link"/>
	</table>
</schema>
