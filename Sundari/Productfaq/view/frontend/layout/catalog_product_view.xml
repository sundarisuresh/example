<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Sundari_Productfaq::css/qa.css"/>
	</head>
	<body>
		<referenceContainer name="content">
			<block class="Sundari\Productfaq\Block\Productfaq" name="productfaq" as="productfaq" template="Sundari_Productfaq::productfaq.phtml"/>
		</referenceContainer>

		<referenceContainer name="product.info.price">
			<block class="Sundari\Productfaq\Block\Productfaq" name="faqsummary" as="faqsummary" after="product.info.review" template="Sundari_Productfaq::productfaqsummary.phtml"/>
		</referenceContainer>
	</body>
</page>



